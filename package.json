{
  "name": "zad-web",
  "version": "1.0.0",
  "description": "Zad Website",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "ci": "cross-env NODE_OPTIONS=--no-deprecation pnpm payload migrate:status && pnpm payload migrate && pnpm next build",
    "build": "cross-env NODE_OPTIONS=--no-deprecation next build",
    "postbuild": "next-sitemap --config next-sitemap.config.cjs",
    "dev": "cross-env NODE_OPTIONS=--no-deprecation next dev",
    "dev:prod": "cross-env NODE_OPTIONS=--no-deprecation rm -rf .next && pnpm build && pnpm start",
    "generate:importmap": "cross-env NODE_OPTIONS=--no-deprecation payload generate:importmap",
    "generate:types": "cross-env NODE_OPTIONS=--no-deprecation payload generate:types",
    "ii": "cross-env NODE_OPTIONS=--no-deprecation pnpm --ignore-workspace install",
    "lint": "cross-env NODE_OPTIONS=--no-deprecation next lint",
    "lint:fix": "cross-env NODE_OPTIONS=--no-deprecation next lint --fix",
    "payload": "cross-env NODE_OPTIONS=--no-deprecation payload",
    "reinstall": "cross-env NODE_OPTIONS=--no-deprecation rm -rf node_modules && rm pnpm-lock.yaml && pnpm --ignore-workspace install",
    "start": "cross-env NODE_OPTIONS=--no-deprecation next start"
  },
  "dependencies": {
    "@iconify-icon/react": "^2.3.0",
    "@payloadcms/db-mongodb": "^3.52.0",
    "@payloadcms/db-postgres": "latest",
    "@payloadcms/db-vercel-postgres": "^3.52.0",
    "@payloadcms/email-resend": "latest",
    "@payloadcms/live-preview-react": "latest",
    "@payloadcms/next": "latest",
    "@payloadcms/payload-cloud": "latest",
    "@payloadcms/plugin-form-builder": "latest",
    "@payloadcms/plugin-nested-docs": "latest",
    "@payloadcms/plugin-redirects": "latest",
    "@payloadcms/plugin-search": "latest",
    "@payloadcms/plugin-seo": "latest",
    "@payloadcms/richtext-lexical": "latest",
    "@payloadcms/storage-s3": "latest",
    "@payloadcms/translations": "latest",
    "@payloadcms/ui": "latest",
    "@radix-ui/react-accordion": "^1.2.12",
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-slot": "^1.2.3",
    "@remixicon/react": "^4.6.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cross-env": "^7.0.3",
    "geist": "^1.4.2",
    "graphql": "^16.11.0",
    "lucide-react": "^0.378.0",
    "motion": "^12.23.12",
    "next": "15.4.5",
    "next-intl": "^4.3.4",
    "next-sitemap": "^4.2.3",
    "payload": "latest",
    "payload-admin-bar": "^1.0.7",
    "prism-react-renderer": "^2.4.1",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-hook-form": "7.45.4",
    "react-use-measure": "^2.1.7",
    "sharp": "0.32.6",
    "tailwind-merge": "^2.6.0",
    "tw-animate-css": "^1.3.7"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@iconify-json/ri": "1.2.5",
    "@iconify/tools": "4.1.2",
    "@iconify/utils": "2.3.0",
    "@tailwindcss/postcss": "^4.1.12",
    "@tailwindcss/typography": "^0.5.16",
    "@types/escape-html": "^1.0.4",
    "@types/node": "22.5.4",
    "@types/react": "19.0.10",
    "@types/react-dom": "19.0.4",
    "copyfiles": "^2.4.1",
    "drizzle-kit": "0.28.0",
    "eslint": "^9.33.0",
    "eslint-config-next": "15.2.0",
    "postcss": "^8.5.6",
    "prettier": "^3.6.2",
    "prettier-plugin-tailwindcss": "^0.6.14",
    "tailwindcss": "^4.1.12",
    "typescript": "5.7.3"
  },
  "packageManager": "pnpm@10.3.0",
  "engines": {
    "node": "^18.20.2 || >=20.9.0"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "sharp"
    ]
  }
}
